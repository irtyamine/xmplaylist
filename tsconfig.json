{
  "compilerOptions": {
    "moduleResolution": "node",
    "lib": ["es2018"],
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "declaration": false,
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "dist"
  },
  "include": [
    "src",
    "models",
    "config",
    "commands",
    "migrations",
    "frontend/channels.ts"
  ],
}
